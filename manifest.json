{
  "manifest_version": 2,
  "name": "Tab-R",
  "version": "1.2",
  "description": "This extension customizes the new tab.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icons/icon.png"
  },
  "web_accessible_resources": [
    "images/*",
    "images.json"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "skrivanek@duck.com",
      "strict_min_version": "91.0"
    }
  }
}

