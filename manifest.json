{
  "manifest_version": 3,
  "name": "Tab-R",
  "version": "1",
  "description": "I don't know.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/*",
        "images.json"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-r@example.com",
      "strict_min_version": "91.0"
    }
  }
}
