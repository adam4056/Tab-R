{
    "manifest_version": 3,
    "name": "Tab-R",
    "version": "1.3",
    "description": "This extension customizes the new tab.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": {
      "48": "icons/icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "images/*",
          "images.json"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
  